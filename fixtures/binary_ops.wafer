public func main() {
	let x = 0;

	x := x + (1 * (1 < 2));
	x := x + (2 * (2 < 1));
	x := x + (4 * (1 > 2));
	x := x + (8 * (2 > 1));
	x := x + (16 * (1 <= 2));
	x := x + (32 * (2 <= 1));
	x := x + (64 * (1 >= 2));
	x := x + (128 * (2 >= 1));
	x := x + (256 * (1 == 1));
	x := x + (512 * (1 == 2));
	x := x + (1024 * (1 != 1));
	x := x + (2048 * (1 != 2));
	x := x + (4096 * (1 and 1));
	x := x + (8192 * (1 and 0));
	x := x + (16384 * (1 or 0));
	x := x + (32768 * (0 or 0));

	x
}
